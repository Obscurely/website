{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "eslint . && ANALYZE=true next build",
    "prebuild": "pnpm run minify",
    "build": "eslint . && next build",
    "predev": "pnpm run minify",
    "dev": "concurrently \"pnpm run minify:watch\" \"next dev --turbopack\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lighthouse-mobile": "pnpx lighthouse http://localhost:3000 --preset=perf --form-factor=mobile --screenEmulation.mobile=true --throttling-method=devtools --output=html --output-path=./lighthouse-mobile-report.html",
    "lighthouse-mobile-unthrottled": "pnpx lighthouse http://localhost:3000 --form-factor=mobile --throttling-method=provided --screenEmulation.mobile=true --emulated-user-agent=\"Mozilla/5.0 (Linux; Android 7.0; Moto G (4)) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/98.0.4758.101 Mobile Safari/537.36\" --output=html --output-path=./lighthouse-mobile-report.html",
    "lint": "eslint .",
    "minify": "terser src/scripts/animations.js -c -m -o public/animations.min.js",
    "minify:watch": "chokidar 'scripts/**/*.js' -c 'npm run minify'",
    "playwright:install": "playwright install",
    "prepare": "husky",
    "start": "next start",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ],
    "*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.958.0",
    "@aws-sdk/credential-providers": "^3.958.0",
    "@floating-ui/react": "^0.27.16",
    "@giscus/react": "^3.1.0",
    "@headlessui/react": "^2.2.9",
    "@icons-pack/react-simple-icons": "^13.8.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tabler/icons-react": "^3.36.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.26",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "isomorphic-dompurify": "^2.34.0",
    "lru-cache": "^11.2.4",
    "next": "^16.1.1",
    "next-mdx-remote": "^5.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-type-animation": "^3.2.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.20.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "validator": "^13.15.26"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@commitlint/types": "^20.2.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@next/bundle-analyzer": "^16.1.1",
    "@next/eslint-plugin-next": "^16.1.1",
    "@playwright/test": "1.54.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/validator": "^13.15.10",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cssnano": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.5.0",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "sass": "^1.97.1",
    "sort-package-json": "^3.6.0",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-tailwindcss": "^1.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-high-performance-animation": "^1.11.0",
    "stylelint-no-unsupported-browser-features": "^8.0.5",
    "stylelint-order": "^7.0.0",
    "stylelint-use-logical-spec": "^5.0.1",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.18",
    "terser": "^5.44.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vitest": "^4.0.16"
  }
}
